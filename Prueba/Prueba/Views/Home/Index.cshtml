@{
    ViewData["Title"] = "Home Page";
    string nombre = "Arrate";
    string saludo = "Hola";
    List<string> compra = new List<string>()
    {
        "Carne", "Pescado","Manzanas","Peras"
    };
    compra.Add("Yogures");
}

<div class="text-center">
    <h1 class="display-4">@saludo @nombre</h1>
    <ul>
        @{ 
            foreach (string item in compra)
            {
                <li>@item</li>
            }
        }
    </ul>
</div>
